{
    "version": 2,
    "builds": [
      {
        "src": "tribe/wsgi.py",
        "use": "@vercel/python",
        "config": { 
          "maxLambdaSize": "15mb", 
          "runtime": "python3.9" 
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "tribe/wsgi.py"
      },
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/media/(.*)",
        "dest": "/media/$1"
      }
    ]
  }
  